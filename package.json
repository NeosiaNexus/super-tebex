{
  "name": "@neosianexus/super-tebex",
  "version": "3.0.4",
  "description": "Tebex Headless SDK optimized for Next.js App Router with TanStack Query",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "build": "bun run build.ts",
    "dev": "bun run build.ts --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "clean": "bun run format && bun run lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "prepublishOnly": "bun run build"
  },
  "peerDependencies": {
    "react": "^18.3.1 || ^19.0.0",
    "react-dom": "^18.3.1 || ^19.0.0",
    "next": "^14.0.0 || ^15.0.0",
    "@tanstack/react-query": "^5.0.0",
    "zustand": "^5.0.0",
    "tebex_headless": "^1.15.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.1",
    "@eslint/eslintrc": "^3.3.1",
    "@tanstack/react-query": "^5.62.0",
    "@tanstack/react-query-devtools": "^5.62.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/bun": "^1.2.18",
    "@types/react": "^19.2.7",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "bun-plugin-dts": "^0.3.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-no-floating-promise": "^2.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jsdom": "^26.1.0",
    "msw": "^2.7.0",
    "next": "^15.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tebex_headless": "^1.15.1",
    "typescript": "^5.7.0",
    "vitest": "^3.2.4",
    "zustand": "^5.0.0"
  },
  "keywords": [
    "tebex",
    "nextjs",
    "react",
    "headless",
    "ecommerce",
    "minecraft",
    "game-server",
    "tanstack-query",
    "react-query"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NeosiaNexus/super-tebex.git"
  },
  "author": "NeosiaNexus <devneosianexus@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/NeosiaNexus/super-tebex#readme",
  "bugs": "https://github.com/NeosiaNexus/super-tebex/issues",
  "engines": {
    "node": ">=18"
  }
}
